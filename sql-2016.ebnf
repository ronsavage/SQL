normal_form ::=
    NFC
  | NFD
  | NFKC
  | NFKD

normalize_function_result_length ::=
    character_length
  | character_large_object_length

specific_type_method ::=
  user_defined_type_value_expression period SPECIFICTYPE
      ( left_paren right_paren )?

binary_value_function ::=
