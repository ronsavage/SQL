  DEFAULT default_option

default_option ::=
    literal
  | datetime_value_function
  | USER
  | CURRENT_USER
  | CURRENT_ROLE
  | SESSION_USER
  | SYSTEM_USER
  | CURRENT_CATALOG
  | CURRENT_SCHEMA
  | CURRENT_PATH
  | implicitly_typed_value_specification
